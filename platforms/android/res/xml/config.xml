<?xml version='1.0' encoding='utf-8'?>
<widget id="org.sonlink.ipcalc" version="0.4.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>IPCalc</name>
    <description>
        Calculate IP Net range for nets
    </description>
    <author email="sonlink.dourden@gmail.com" href="http://github.com/son-link">
        Alfonso Saavedra "Son Link"
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="landscape" />
</widget>
